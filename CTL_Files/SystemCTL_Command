sudo nano /etc/systemd/system/DynaApp.service

sudo systemctl daemon-reload

sudo systemctl enable DynaApp.service

sudo systemctl start DynaApp.service

sudo systemctl status DynaApp.service

journalctl -u DynaApp.service

